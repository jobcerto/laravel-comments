{
    "name": "jobcerto/laravel-comments",
    "description": "Laravel Comments System",
    "keywords": [
        "jobcerto",
        "laravel-comments"
    ],
    "homepage": "https://github.com/jobcerto/laravel-comments",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Eduardo Geschonke",
            "email": "edu@jobcerto.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Jobcerto\\LaravelComments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jobcerto\\LaravelComments\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jobcerto\\LaravelComments\\LaravelCommentsServiceProvider"
            ],
            "aliases": {
                "LaravelComments": "Jobcerto\\LaravelComments\\LaravelCommentsFacade"
            }
        }
    }
}
